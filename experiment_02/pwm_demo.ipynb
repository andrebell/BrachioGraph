    "from ipywidgets import interact\n",
    "\n",
    "@interact(width=(0.027,0.114,0.005))\n",
    "def pwm(width):\n",
    "    t = linspace(0,5,1000)\n",
    "    y = 1.0* (t % 1 < width)\n",
    "    plot(t,y)"
